n\">\n    <my-header></my-header>\n    <van-sidebar v-model=\"activeKey\">\n      <van-sidebar-item :title=\"item\" v-for=\"(item,index) in ary\" ref=\"qqq\" @click=\"fn\" :key=\"item\"/>\n    </van-sidebar>\n    <main-box></main-box>\n  </div>\n</template>\n<script>\n// @ is an alias to /src\nimport header from \"./header.vue\";\nimport mainBox from \"./countent.vue\";\nexport default {\n  name: \"classify\",\n  data() {\n    return {\n      activeKey: 0,\n      ary: [\n        \"女装\",\n        \"男装\",\n        \"母婴\",\n        \"家具\",\n        \"生鲜\",\n        \"饮料\",\n        \"玩具\",\n        \"箱包\",\n        \"珠宝\",\n        \"内衣\",\n        \"配饰\"\n      ],\n      flag: false\n    };\n  },\n  created() {},\n  mounted() {\n    //渲染完之后\n    this.$refs.qqq[0].$el.style.color = \"red\";\n  },\n  components: {\n    \"my-header\": header,\n    \"main-box\": mainBox\n  },\n  methods: {\n    fn(Index) {\n      let a = this.$refs.qqq;\n      a.forEach((item, index) => {\n        if (Index == index) {\n          item.$el.style.color = \"red\";\n        } else {\n          item.$el.style.color = \"black\";\n        }\n      });\n    }\n  },\n  computed: {}\n};\n</script>\n<style lang=\"less\" scoped>\n.main {\n  .van-sidebar {\n    margin-top: 57px;\n    overflow: hidden;\n    // border:1px solid lightgray;\n  }\n  .van-sidebar-item {\n    border-left: none;\n  }\n}\n</style>"],"sourceRoot":"src/components/classify"}]}